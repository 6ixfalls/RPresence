{
  "applicationId": "me.sixfallls.rpresence",
  "version": "2.0.0",
  "defaultMode": "window",
  "port": 0,
  "documentRoot": "/resources/",
  "url": "/",
  "enableServer": true,
  "enableNativeAPI": true,
  "tokenSecurity": "one-time",
  "logging": {
    "enabled": true,
    "writeToLogFile": true
  },
  "nativeAllowList": [
    "app.*",
    "os.*",
    "debug.log"
  ],
  "modes": {
    "window": {
      "title": "rpresence",
      "icon": "/ico/logo.png",
      "hidden": true,
      "enableInspector": true,
      "exitProcessOnClose": false
    }
  },
  "cli": {
    "binaryName": "rpresence",
    "resourcesPath": "/resources/",
    "extensionsPath": "/extensions/",
    "clientLibrary": "/resources/neutralino.js",
    "binaryVersion": "4.7.0",
    "clientVersion": "3.6.0"
  }
}